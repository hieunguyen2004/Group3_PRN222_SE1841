@model string

<h2>Forget Password</h2>

<form asp-action="ForgetPassword" method="post">
    <div>
        <label>Username or Email:</label>
        <input type="text" name="usernameOrEmail" required />
    </div>
    <button type="submit">Submit</button>
</form>

@if (!string.IsNullOrEmpty(Model))
{
    <p>@Model</p>
}
